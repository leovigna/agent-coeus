{
  "name": "@coeus-agent/ops-db",
  "version": "0.1.0",
  "private": true,
  "keywords": [],
  "homepage": "https://github.com/leovigna/agent-coeus",
  "repository": {
    "type": "git",
    "url": "https://github.com/leovigna/agent-coeus.git"
  },
  "copyright": "Copyright 2024 Leo Vigna",
  "license": "MIT",
  "author": {
    "name": "Leo Vigna",
    "email": "leo@coeus-agent.io",
    "url": "https://github.com/leovigna"
  },
  "type": "module",
  "files": [
    "_cjs/*",
    "_esm/*",
    "_types/*",
    "src/*",
    "LICENSE",
    "README.md"
  ],
  "main": "./_cjs/lib/index.cjs",
  "module": "./_esm/lib/index.mjs",
  "types": "./_types/index.d.ts",
  "exports": {
    ".": {
      "types": "./_types/index.d.ts",
      "require": "./_cjs/lib/index.js",
      "import": "./_esm/lib/index.js"
    }
  },
  "scripts": {
    "build": "npm-run-all -p tsc esbuild",
    "build:watch": "npm-run-all -p tsc:watch esbuild:watch",
    "clean": "rimraf _cjs _esm _types .turbo",
    "depcheck": "depcheck",
    "esbuild": "node esbuild.config.mjs",
    "esbuild:watch": "ESBUILD_WATCH=true node esbuild.config.mjs",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "madge": "madge src/index.ts -c",
    "test": "pnpm run test:ci",
    "test:ci": "vitest --run",
    "test:watch": "vitest",
    "tsc": "tsc",
    "tsc:trace": "tsc --generateTrace lib/trace && analyze-trace lib/trace",
    "tsc:watch": "tsc -w",
    "db:push": "tsx node_modules/drizzle-kit/bin.cjs push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.44.5"
  },
  "devDependencies": {
    "@coeus-agent/esbuild-config": "workspace:*",
    "@coeus-agent/eslint-config": "workspace:*",
    "@coeus-agent/tsconfig": "workspace:*",
    "@coeus-agent/vite-config": "workspace:*",
    "@types/node": "^22.13.10",
    "@typescript/analyze-trace": "^0.10.1",
    "depcheck": "^1.4.7",
    "drizzle-kit": "^0.31.4",
    "madge": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.5",
    "typescript": "5.8.3",
    "vite": "^6.3.1",
    "vitest": "^3.0.8"
  }
}