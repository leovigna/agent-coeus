{
  "name": "@coeus-agent/mcp-tools-zep",
  "version": "0.1.0",
  "private": true,
  "keywords": [],
  "homepage": "https://github.com/leovigna/starter-monorepo",
  "repository": {
    "type": "git",
    "url": "https://github.com/leovigna/starter-monorepo.git"
  },
  "copyright": "Copyright 2024 Leo Vigna",
  "license": "MIT",
  "author": {
    "name": "Leo Vigna",
    "email": "leo@coeus-agent.io",
    "url": "https://github.com/leovigna"
  },
  "type": "module",
  "files": [
    "_cjs/*",
    "_esm/*",
    "_types/*",
    "src/*",
    "LICENSE",
    "README.md"
  ],
  "main": "./_cjs/lib/index.cjs",
  "module": "./_esm/lib/index.mjs",
  "types": "./_types/index.d.ts",
  "exports": {
    ".": {
      "types": "./_types/index.d.ts",
      "require": "./_cjs/lib/index.js",
      "import": "./_esm/lib/index.js"
    },
    "./sdk": {
      "types": "./_types/sdk/index.d.ts",
      "require": "./_cjs/lib/sdk/index.js",
      "import": "./_esm/lib/sdk/index.js"
    },
    "./sdk/edge": {
      "types": "./_types/sdk/edge/index.d.ts",
      "require": "./_cjs/lib/sdk/edge/index.js",
      "import": "./_esm/lib/sdk/edge/index.js"
    },
    "./sdk/edge/*": {
      "types": "./_types/sdk/edge/*.d.ts",
      "require": "./_cjs/lib/sdk/edge/*.js",
      "import": "./_esm/lib/sdk/edge/*.js"
    },
    "./sdk/episode": {
      "types": "./_types/sdk/episode/index.d.ts",
      "require": "./_cjs/lib/sdk/episode/index.js",
      "import": "./_esm/lib/sdk/episode/index.js"
    },
    "./sdk/episode/*": {
      "types": "./_types/sdk/episode/*.d.ts",
      "require": "./_cjs/lib/sdk/episode/*.js",
      "import": "./_esm/lib/sdk/episode/*.js"
    },
    "./sdk/graph": {
      "types": "./_types/sdk/graph/index.d.ts",
      "require": "./_cjs/lib/sdk/graph/index.js",
      "import": "./_esm/lib/sdk/graph/index.js"
    },
    "./sdk/graph/*": {
      "types": "./_types/sdk/graph/*.d.ts",
      "require": "./_cjs/lib/sdk/graph/*.js",
      "import": "./_esm/lib/sdk/graph/*.js"
    }
  },
  "scripts": {
    "build": "npm-run-all -p tsc esbuild",
    "build:watch": "npm-run-all -p tsc:watch esbuild:watch",
    "clean": "rimraf _cjs _esm _types .turbo",
    "depcheck": "depcheck",
    "esbuild": "node esbuild.config.mjs",
    "esbuild:watch": "ESBUILD_WATCH=true node esbuild.config.mjs",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "madge": "madge src/index.ts -c",
    "test": "pnpm run test:ci",
    "test:ci": "vitest --run",
    "test:watch": "vitest",
    "tsc": "tsc",
    "tsc:trace": "tsc --generateTrace lib/trace && analyze-trace lib/trace",
    "tsc:watch": "tsc -w"
  },
  "dependencies": {
    "@coeus-agent/mcp-tools-base": "workspace:*",
    "@coeus-agent/mcp-tools-logto": "workspace:*",
    "http-errors-enhanced": "^3.0.2",
    "lodash-es": "^4.17.21",
    "uuid": "^13.0.0"
  },
  "peerDependencies": {
    "@getzep/zep-cloud": "^3.4.1",
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@trpc/server": "^11.5.1",
    "trpc-to-openapi": "^2.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@coeus-agent/esbuild-config": "workspace:*",
    "@coeus-agent/eslint-config": "workspace:*",
    "@coeus-agent/tsconfig": "workspace:*",
    "@coeus-agent/vite-config": "workspace:*",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.10",
    "@typescript/analyze-trace": "^0.10.1",
    "depcheck": "^1.4.7",
    "madge": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "5.8.3",
    "vite": "^6.3.1",
    "vitest": "^3.0.8"
  }
}