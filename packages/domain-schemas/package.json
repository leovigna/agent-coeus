{
  "name": "@coeus-agent/domain-schemas",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm run clean && pnpm run build:esm && pnpm run build:cjs",
    "build:esm": "esbuild src/index.ts --bundle --platform=node --target=node20 --format=esm --outfile=dist/index.js",
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --target=node20 --format=cjs --outfile=dist/index.cjs",
    "clean": "rimraf dist",
    "dev": "pnpm run build:esm --watch",
    "lint": "eslint .",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@coeus-agent/esbuild-config": "workspace:*",
    "@coeus-agent/eslint-config": "workspace:*",
    "@coeus-agent/tsconfig": "workspace:*",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}