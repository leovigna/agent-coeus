{
  "name": "@coeus-agent/coeus-mcp",
  "version": "0.1.0",
  "private": true,
  "keywords": [],
  "homepage": "https://github.com/leovigna/starter-monorepo",
  "repository": {
    "type": "git",
    "url": "https://github.com/leovigna/starter-monorepo.git"
  },
  "copyright": "Copyright 2024 Leo Vigna",
  "license": "MIT",
  "author": {
    "name": "Leo Vigna",
    "email": "leo@coeus-agent.io",
    "url": "https://github.com/leovigna"
  },
  "type": "module",
  "files": [
    "_cjs/*",
    "_esm/*",
    "_types/*",
    "src/*",
    "LICENSE",
    "README.md"
  ],
  "main": "./_cjs/lib/index.cjs",
  "module": "./_esm/lib/index.mjs",
  "types": "./_types/index.d.ts",
  "exports": {
    ".": {
      "types": "./_types/index.d.ts",
      "require": "./_cjs/lib/index.js",
      "import": "./_esm/lib/index.js"
    }
  },
  "scripts": {
    "start": "node ./_esm/lib/server.js",
    "dev": "tsx --watch ./src/server.ts",
    "build": "npm-run-all -p tsc esbuild",
    "build:watch": "npm-run-all -p tsc:watch esbuild:watch",
    "clean": "rimraf _cjs _esm _types .turbo",
    "depcheck": "depcheck",
    "esbuild": "node esbuild.config.mjs",
    "esbuild:watch": "ESBUILD_WATCH=true node esbuild.config.mjs",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "madge": "madge src/index.ts -c",
    "test": "pnpm run test:ci",
    "test:ci": "vitest --run",
    "test:watch": "vitest",
    "tsc": "tsc",
    "tsc:trace": "tsc --generateTrace lib/trace && analyze-trace lib/trace",
    "tsc:watch": "tsc -w"
  },
  "dependencies": {
    "@getzep/zep-cloud": "^3.4.1",
    "@modelcontextprotocol/sdk": "^1.17.4",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "mcp-auth": "^0.1.1",
    "morgan": "^1.10.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@coeus-agent/esbuild-config": "workspace:*",
    "@coeus-agent/eslint-config": "workspace:*",
    "@coeus-agent/tsconfig": "workspace:*",
    "@coeus-agent/vite-config": "workspace:*",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.13.10",
    "@typescript/analyze-trace": "^0.10.1",
    "depcheck": "^1.4.7",
    "dotenv": "^16.4.7",
    "madge": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.5",
    "typescript": "5.8.3",
    "vite": "^6.3.1",
    "vitest": "^3.0.8"
  }
}